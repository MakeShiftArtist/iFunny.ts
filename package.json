{
	"name": "ifunny.ts",
	"version": "0.2.0",
	"description": "Official ifunny-co ifunny API wrapper written in TypeScript.",
	"main": "dist",
	"types": "types",
	"scripts": {
		"test": "jest",
		"changelog": "auto-changelog -p  && git add CHANGELOG.md && git commit -m \"Update CHANGELOG.md\"",
		"changelog-debug": "auto-changelog -p --template json --output changelog-data.json",
		"type": "tsc",
		"build": "npx tsc && npm run type",
		"update-deps": "npx npm-check-updates -u && npm install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ifunny-co/ifunny.ts"
	},
	"keywords": [
		"ifunny",
		"typescript",
		"api",
		"library",
		"ifunny.co"
	],
	"author": "",
	"license": "CC0-1.0",
	"bugs": {
		"url": "https://github.com/ifunny-co/iFunny.ts/issues"
	},
	"homepage": "https://github.com/ifunny-co/iFunny.ts#readme",
	"devDependencies": {
		"@types/jest": "^28.0.8",
		"@types/node": "^18.7.13",
		"auto-changelog": "^2.4.0",
		"jest": "^28.0.8",
		"ts-jest": "^28.0.8",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.2"
	},
	"auto-changelog": {
		"template": "changelog-template.hbs",
		"hideEmptyReleases": true,
		"commitLimit": false,
		"unreleased": true,
		"replaceText": {
			"[Ff]eat:": "",
			"[Ff]ix:": "",
			"[Bb]reak:": "",
			"[Dd]oc:": "",
			"[Cc]hore:": ""
		}
	},
	"dependencies": {
		"@discordjs/collection": "^1.1.0",
		"@ifunny/ifunny-api-types": "^0.7.1",
		"axios": "^0.27.2",
		"eventemitter3": "^4.0.7"
	}
}
